    "def theoretical(n, x_max):\n",
    "    if x_max < 1 or x_max > 6:\n",
    "        return 0\n",
    "    return (x_max ** n - (x_max - 1) ** n) / 6 ** n\n",
    "\n",
    "\n",
    "def practice(n, x_max):\n",
    "    if x_max < 1 or x_max > 6:\n",
    "        return 0\n",
    "    count = 0\n",
    "    space = range(1, 7)\n",
    "    for sim in range(10000):\n",
    "        tmp = np.random.choice(space, n)\n",
    "        if max(tmp) == x_max:\n",
    "            count += 1\n",
    "    return count / 10000\n",
